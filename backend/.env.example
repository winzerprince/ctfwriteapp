# Use this as a template to create a .env file in the same directory as this file.
# JWT Secret for authentication
JWT_SECRET=your_secure_jwt_secret_key_here

# GitHub Repository URL (used for uploading writeups)
GITHUB_REPO_URL=https://github.com/winzerprince/writeapp.git

# Backend server port
PORT=5001

# Temp directory for git operations
GITHUB_REPO_PATH=./temp_git_repos/writeapp-repo

# Electron running flag (set to 'true' when running in Electron)
ELECTRON_RUN=false
